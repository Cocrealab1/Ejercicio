<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>
        <%=title%>
    </title>
</head>

<body>
    <!--Mostrar el mensaje flash -->
    <% for (var i in messages){%>
        <div class='flash'>
            <%message[i] %>
        </div>
        <% } %>

            <form action="/signup" method="post">
                <div>
                    <label>Usuario:</label>
                    <input type="text" name="usuario" />
                </div>
                <div>
                    <label>Contaseña:</label>
                    <input type="text" name="contraña" />
                </div>
                <div>
                    <input type="submit" name="Identificarse" />
                </div>
            </form>

</body>

</html>
